<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="oval">
    <solid android:color="#199fff"/>
    <!--center color #105575-->
    <!--start color #082946-->
    <!--end color #06545C-->
    <gradient
        android:angle="45"
        android:centerX="40%"
        android:centerColor="#543A14"
        android:startColor="#481E14"
        android:endColor="#706233"
        android:type="linear" />
    <size
        android:width="72dp"
        android:height="72dp"/>
    <padding
        android:top="16dp"
        android:left="16dp"
        android:bottom="16dp"
        android:right="16dp"
        />

</shape>
    </item>
    <item
        android:height="4dp"
        android:left="@dimen/small_margin"
        android:right="@dimen/small_margin"
        android:gravity="center">
        <shape android:shape="rectangle">
            <corners android:radius="1dp" />
            <!--center color #F4F5FF-->
            <!--start color #DFE0EF-->
            <!--end color #DCE3ED-->
            <gradient
                android:angle="45"
                android:centerColor="#F4F5FF"
                android:centerX="50%"
                android:endColor="#DCE3ED"
                android:startColor="#DFE0EF"
                android:type="linear" />
        </shape>
    </item>
    <item
        android:width="4dp"
        android:top="@dimen/small_margin"
        android:bottom="@dimen/small_margin"
        android:gravity="center">
        <shape android:shape="rectangle">
            <corners android:radius="1dp" />
            <!--center color #F4F5FF-->
            <!--start color #DFE0EF-->
            <!--end color #DCE3ED-->
            <gradient
                android:angle="45"
                android:centerColor="#F4F5FF"
                android:centerX="50%"
                android:endColor="#DCE3ED"
                android:startColor="#DFE0EF"
                android:type="linear" />

        </shape>
    </item>
</layer-list>

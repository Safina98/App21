<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="oval">
    <solid android:color="#199fff"/>
    <gradient
        android:angle="45"
        android:centerX="40%"
        android:centerColor="#105575"
        android:startColor="#082946"
        android:endColor="#06545C"
        android:type="linear" />
    <!--
android:angle="45"
        android:centerColor="#FFCDD2"
        android:centerX="60%"
        android:endColor="#DCEDC8"
        android:startColor="#F8BBD0"
        android:type="radial"
    -->
    <size
        android:width="72dp"
        android:height="72dp"/>
    <padding
        android:top="16dp"
        android:left="16dp"
        android:bottom="16dp"
        android:right="16dp"
        />

</shape>
    </item>
    <item
        android:height="5dp"
        android:gravity="center">
        <shape android:shape="rectangle">
            <corners android:radius="1dp" />
            <gradient
                android:angle="45"
                android:centerColor="#F4F5FF"
                android:centerX="50%"
                android:endColor="#DCE3ED"
                android:startColor="#DFE0EF"
                android:type="linear" />
        </shape>
    </item>
    <item
        android:width="5dp"
        android:gravity="center">
        <shape android:shape="rectangle">
            <corners android:radius="1dp" />
            <gradient
                android:angle="45"
                android:centerColor="#F4F5FF"
                android:centerX="50%"
                android:endColor="#DCE3ED"
                android:startColor="#DFE0EF"
                android:type="linear" />

        </shape>
    </item>
</layer-list>
